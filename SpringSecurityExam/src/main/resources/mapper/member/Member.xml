<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd">
<mapper namespace="member">
	<select id="selectUserById" parameterType="java.lang.String" resultType="com.sse.app.member.model.MemberUserDetails">
		SELECT *
		FROM MEMBER
		WHERE ID=#{id}
	</select>
</mapper>